{
  "type": "bubble",
  "header": {
    "type": "box",
    "layout": "vertical",
    "contents": [
      {
        "type": "text",
        "text": "Wolf Game Terbaru",
        "weight": "bold"
      }
    ]
  },
  "hero": {
    "type": "image",
    "url": "%1$s",
    "size": "full",
    "aspectRatio": "1.51:1",
    "aspectMode": "cover"
  },
  "body": {
    "type": "box",
    "layout": "vertical",
    "contents": [
      {
        "type": "text",
        "text": "%2$s",
        "size": "md",
        "color": "#c9302c",
        "weight": "bold",
        "wrap": true
      },
      {
        "type": "text",
        "text": "oleh: %3$s",
        "size": "sm",
        "color": "#c9302c"
      },
      {
        "type": "text",
        "text": "%4$s",
        "size": "sm",
        "wrap": true,
        "margin": "lg"
      },
      {
        "type": "box",
        "layout": "horizontal",
        "margin": "xl",
        "contents": [
          {
            "type": "text",
            "text": "Rating",
            "size": "sm",
            "color": "#aaaaaa",
            "flex": 1
          },
          {
            "type": "text",
            "text": "%5$s",
            "size": "sm",
            "color": "#666666",
            "flex": 4
          }
        ]
      },
      {
        "type": "box",
        "layout": "horizontal",
        "margin": "md",
        "contents": [
          {
            "type": "text",
            "text": "Jml Rating",
            "size": "sm",
            "color": "#aaaaaa",
            "flex": 1
          },
          {
            "type": "text",
            "text": "%6$s Orang",
            "size": "sm",
            "color": "#666666",
            "flex": 4
          }
        ]
      },
      {
        "type": "box",
        "layout": "horizontal",
        "margin": "md",
        "contents": [
          {
            "type": "text",
            "text": "Metacritic",
            "size": "sm",
            "color": "#aaaaaa",
            "flex": 1
          },
          {
            "type": "text",
            "text": "%7$s",
            "size": "sm",
            "color": "#666666",
            "flex": 4
          }
        ]
      },
      {
        "type": "box",
        "layout": "horizontal",
        "margin": "md",
        "contents": [
          {
            "type": "text",
            "text": "Playtime",
            "size": "sm",
            "color": "#aaaaaa",
            "flex": 1
          },
          {
            "type": "text",
            "text": "%8$s",
            "size": "sm",
            "color": "#666666",
            "wrap": true,
            "flex": 4
          }
        ]
      }
    ]
  },
  "footer": {
    "type": "box",
    "layout": "horizontal",
    "spacing": "sm",
    "contents": [
      {
        "type": "button",
        "style": "secondary",
        "action": {
          "type": "uri",
          "label": "Lihat",
          "uri": "%10$s"
        }
      }
    ]
  }
}